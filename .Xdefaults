URxvt*color10:          #c5f779
! !yellow text
URxvt*color3:           #cdcda1
URxvt*color11:          #ffffaf
! !blue text
URxvt*color4:           #86a2be
URxvt*color12:          #98afd9
! !magenta text
URxvt*color5:           #d9b798
URxvt*color13:          #d7d998
! !cyan text
URxvt*color6:           #a1b5cd
URxvt*color14:          #a1b5cd
URxvt*color7:           #ffffff
URxvt*color15:          #dedede
