!Xft.dpi:        100
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.autohint:  true
Xft.hintstyle:  hintfull


Xcursor.size: 16

URxvt*termName:          URxvt
URxvt*imLocale:          en_US.UTF-8
URxvt*foreground:        grey90
URxvt*cursorBlink:       true
URxvt*depth:             32
URxvt*font:              xft:Droid Sans Mono:style=Regular:antialias=true;size=10, \
                         xft:Inconsolata for Powerline:antialias=true;size=10,\
                         xft:DejaVu Sans Mono:style=Book:antialias=true:size=10
URxvt*boldFont:          xft:Droid Sans Mono:antialias=true;size=11, \
                         xft:Inconsolata for Powerline:antialias=true;size=11, \
                         xft:DejaVu Sans Mono:style=Book:antialias=true:size=11
URxvt*background:        #242424
URxvt*geometry:          90x30
URxvt.inheritPixmap:     false
URxvt*letterSpace:       -1
URxvt.xftAntialias:      true
URxvt*loginShell:        true
URxvt*saveLines:         4000
URxvt*scrollBar:         false
URxvt*tintColor:         #ffffff
URxvt*underlineColor:    #86a2be
URxvt*matcher.button:    1
URxvt*matcher.pattern.1: \bwww\.[\w-]\.[\w./?&@#-]*[\w/-]
URxvt*urlLauncher:       chromium
URxvt*iconFile:          /usr/share/icons/oxygen/16x16/apps/utilities-terminal.png
URxvt.perl-ext-common:   default,matcher,tabbedex,font-size
URxvt.tabbed.autohide:   true
URXvt.tabbed.reopen-on-close: yes
!URxvt.tabbed.new-button: true
URxvt.iso14755:          false
URxvt.iso14755_52:       false
URxvt.keysym.C-Up:       perl:font-size:increase
URxvt.keysym.C-Down:     perl:font-size:decrease
URxvt.keysym.C-S-Up:     perl:font-size:incglobal
URxvt.keysym.C-S-Down:   perl:font-size:decglobal

!
! !black text
URxvt*color0:           #242424
URxvt*color8:           #cdb5cd
! !red text
URxvt*color1:           #c11b17
URxvt*color9:           #c11b17
! !green text
URxvt*color2:           #97b26b
URxvt*color10:          #c5f779
! !yellow text
URxvt*color3:           #cdcda1
URxvt*color11:          #ffffaf
! !blue text
URxvt*color4:           #86a2be
URxvt*color12:          #98afd9
! !magenta text
URxvt*color5:           #d9b798
URxvt*color13:          #d7d998
! !cyan text
URxvt*color6:           #a1b5cd
URxvt*color14:          #a1b5cd
URxvt*color7:           #ffffff
URxvt*color15:          #dedede
