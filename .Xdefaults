Xft.hinting: 1
Xft.hintstyle: hintfull
Xcursor.size: 16
Xcursor.theme: Oxygen_White


! rxvt-unicode ---------------------------------------------------------------

urxvt*foreground:        grey90
urxvt*cursorBlink:       true
urxvt*depth:             32
urxvt*font:              xft:Inconsolata:pixelsize=14
urxvt*boldFont:          xft:Inconsolata:pixelsize=14
urxvt*background:        #000000
urxvt*geometry:          90x30
urxvt*loginShell:        true
urxvt*saveLines:         4000
urxvt*scrollBar:         false
urxvt*termName:          rxvt
urxvt*tintColor:         #ffffff
urxvt*underlineColor:    #86a2be
urxvt*perl-ext-common:   default,matcher
urxvt*matcher.button:    1
urxvt*matcher.pattern.1: \bwww\.[\w-]\.[\w./?&@#-]*[\w/-]
urxvt*urlLauncher:       firefox
urxvt*iconFile:          /usr/share/icons/oxygen/16x16/apps/utilities-terminal.png
urxvt*termName:          screen-256color
urxvt.perl-ext-common:   default,tabbedex

