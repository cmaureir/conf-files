Xft.dpi:        75
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.autohint:  true
Xft.hintstyle:  hintfull


Xcursor.size: 16

URxvt*termName:          URxvt
URxvt*imLocale:          en_US.UTF-8
URxvt*foreground:        grey90
URxvt*cursorBlink:       true
URxvt*depth:             32
URxvt*font:              xft:Droid Sans Mono:style=Regular:antialias=true;pixelsize=11, \
                         xft:Inconsolata for Powerline:antialias=true;size=11,\
                         xft:DejaVu Sans Mono:style=Book:antialias=true:size=11
URxvt*boldFont:          xft:Droid Sans Mono:antialias=true;size=13, \
                         xft:Inconsolata for Powerline:antialias=true;size=13, \
                         xft:DejaVu Sans Mono:style=Book:antialias=true:size=13
URxvt*background:        #242424
URxvt*geometry:          90x30
URxvt.inheritPixmap:     false
URxvt*letterSpace:       -1
URxvt.xftAntialias:      true
URxvt*loginShell:        true
URxvt*saveLines:         4000
URxvt*scrollBar:         false
URxvt*tintColor:         #ffffff
URxvt*underlineColor:    #86a2be
URxvt*perl-ext-common:   default,matcher
URxvt*matcher.button:    1
URxvt*matcher.pattern.1: \bwww\.[\w-]\.[\w./?&@#-]*[\w/-]
URxvt*urlLauncher:       chromium
URxvt*iconFile:          /usr/share/icons/oxygen/16x16/apps/utilities-terminal.png
URxvt.perl-ext-common:   default
